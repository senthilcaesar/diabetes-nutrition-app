# OpenAI API Key for the RAG system
# Replace with your actual API key from https://platform.openai.com/api-keys
# You can use either OPENAI_API_KEY or OPENAI_KEY
OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_KEY=your_openai_api_key_here  # Alternative variable name

# OpenAI Model to use for the RAG system
OPENAI_MODEL=gpt-4o

# Optional: API host (default is "openai")
# API_HOST=openai

# Pinecone API Key and Environment for the RAG system
# Replace with your actual API key from https://app.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here  # e.g., gcp-starter, us-west1-gcp, etc.
